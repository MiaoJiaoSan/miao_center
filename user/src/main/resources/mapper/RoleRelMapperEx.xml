<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.miaojiaosan.user.dal.mapperex.RoleRelMapperEx">
    <resultMap id="BaseResultMap" type="com.miaojiaosan.user.dal.dao.RoleRelDAO"
               extends="com.miaojiaosan.user.dal.mapper.RoleRelMapper.BaseResultMap"/>

    <insert id="insert" parameterType="com.miaojiaosan.user.dal.dao.RoleRelDAO">
        insert into role_rel
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="id != null">
                id,
            </if>
            user_id, role_id,
            <if test="version != null">
                version,
            </if>
            <if test="modify != null">
                modify,
            </if>
            <if test="modifyTime != null">
                modify_time,
            </if>
        </trim>
        values
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="id != null">
                #{id,jdbcType=BIGINT},
            </if>
            #{userId,jdbcType=BIGINT}, #{roleId,jdbcType=BIGINT},
            <if test="version != null">
                #{version,jdbcType=BIGINT},
            </if>
            <if test="modify != null">
                #{modify,jdbcType=BIGINT},
            </if>
            <if test="modifyTime != null">
                #{modifyTime,jdbcType=TIMESTAMP},
            </if>
        </trim>
    </insert>

    <select id="byUserId"  resultMap="BaseResultMap" parameterType="java.lang.Long">
        select id, user_id, role_id, version, modify, modify_time
    from role_rel where user_id = #{userId}
    </select>

</mapper>